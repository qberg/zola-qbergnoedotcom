{
  "buildCommand": "curl -sL https://github.com/getzola/zola/releases/download/v0.19.2/zola-v0.19.2-x86_64-unknown-linux-gnu.tar.gz | tar zxv && ./zola build",
  "outputDirectory": "public",
  "framework": "zola",
  "headers": [
    {
      "source": "/atom.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/atom+xml"
        }
      ]
    },
    {
      "source": "/rss.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/rss+xml"
        }
      ]
    }
  ]
}
